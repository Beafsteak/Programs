<!--?xml version="1.0"?-->
<settings>
   
  <!-- for detailed info about the settings see http://webgrabplus.com/node/30 -->
   
  <filename>/var/www/kodi/guides/news.xml</filename>
  <mode></mode>
  <postprocess run="n" grab="y">mdb</postprocess>
  <proxy>automatic</proxy>
  <user-agent>Mozilla/5.0 (Windows NT 6.1; WOW64; rv:29.0) Gecko/20100101 Firefox/29.0</user-agent>
  <logging>on</logging>
  <retry time-out="5">4</retry>
  <skip>noskip</skip>
  <timespan>1</timespan>
  <update>f</update>
 
  <!-- replace the next dummy channel entry with the channels you want -->
  <!-- see http://webgrabplus.com/node/94 for the available sites/channels -->
<channels>    
	<channel update="i" site="tv.com" site_id="69031481" xmltv_id="Fox News Channel HDTV (FNCHD)">Fox News</channel>
	<channel update="i" site="tvguide.co.uk" site_id="66" xmltv_id="BBC News">BBC News</channel>
	<channel update="i" site="locatetv.com" site_id="/listings/cnn-hd" xmltv_id="CNN HD">CNN</channel>
	<channel update="i" site="locatetv.com" site_id="/listings/cnbc" xmltv_id="CNBC">CNBC</channel>
	<channel update="i" site="tvguide.co.uk" site_id="257" xmltv_id="Sky News">Sky News</channel>		
  </channels>
</settings>
